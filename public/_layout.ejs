

<!DOCTYPE html>
<html lang="en" xmlns="http://www.w3.org/1999/html">

<%- partial("partial/_head") %>


<body>
    <%- partial("partial/_header") %>
    <div class="container">
        <div class="jumbotron">
            <h1>GC Computer Science</h1>
            <p class="lead <%- current.source %>">Train your mind, create your career.</p>
            <p><a class="btn btn-lg btn-success <%- current.source %>" href="http://www.georgetowncollege.edu/admissions/#/home" role="button">Apply</a></p>
        </div>

        <% if ( current.path.includes("posts") ) { %>
        <h2 class="blog-title"><%- title %></h2>
        <p class="blog-subtitle"><%- date %></p>
        <% } %>

        <% if ( current.path.includes("courses") ) { %>
        <h2><%- name %></h2>
        <% } %>

        <% if ( current.path.includes("events") ) { %>
        <h2 class="blog-title"><%- name %></h2>
        <% let place = location ? ", " + location : "" %>
        <p class="blog-subtitle"><%- date %><%- place %></p>
        <% } %>

        <%- yield %>

        <div class="quotation">
            <h2>Random Programming Quote</h2>
            <blockquote>
            </blockquote>
        </div>
    </div>
</body>

<%- partial("partial/_footer") %>
<%- partial("partial/_scripts") %>

</html>